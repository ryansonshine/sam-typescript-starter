{
  "name": "sam-typescript-starter",
  "version": "1.0.0",
  "description": "AWS SAM TypeScript Starter",
  "author": "Ryan Sonshine",
  "scripts": {
    "start": "IS_LOCAL=true npm run build && concurrently 'webpack --watch' 'sam local start-api'",
    "deploy": "sam deploy --guided",
    "build": "npm run clean && webpack",
    "lint": "eslint src/ --fix",
    "test:int": "jest -c jest.config.int.js --ci",
    "test:watch": "jest --watch",
    "test": "jest --ci --coverage",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf .aws-sam/"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.66",
    "@types/jest": "^26.0.19",
    "@types/node": "^12.19.9",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "aws-sam-webpack-plugin": "^0.8.1",
    "aws-sdk": "^2.811.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "fork-ts-checker-webpack-plugin": "^6.0.7",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.12",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "webpack": "^5.10.3",
    "webpack-cli": "^4.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": "eslint --cache --cache-location .eslint_cache --fix"
  }
}
